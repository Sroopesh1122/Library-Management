<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UserController</servlet-name>
  	<display-name>UserController</display-name>
  	<description></description>
  	<servlet-class>com.library.controllers.UserController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BookController</servlet-name>
  	<display-name>BookController</display-name>
  	<description></description>
  	<servlet-class>com.library.controllers.BookController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CategoryController</servlet-name>
  	<display-name>CategoryController</display-name>
  	<description></description>
  	<servlet-class>com.library.controllers.CategoryController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Uploader</servlet-name>
  	<display-name>Uploader</display-name>
  	<description></description>
  	<servlet-class>com.library.controllers.Uploader</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserController</servlet-name>
  	<url-pattern>/user</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BookController</servlet-name>
  	<url-pattern>/bookController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CategoryController</servlet-name>
  	<url-pattern>/catergoryController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Uploader</servlet-name>
  	<url-pattern>/Uploader</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>user/SiginIn.jsp</welcome-file>
  </welcome-file-list>
  
</web-app>
